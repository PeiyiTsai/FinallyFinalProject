@model IEnumerable<FinalProject.Viewmodel.OrderDateGroup>

@{
    ViewBag.Title = "Student Body Statistics";
}

    <body background="http://sheng.phy.nknu.edu.tw/shengwjs08MY/back-151.gif">
                      
                            <h2>會員購買日期</h2>

                            <table class="table">
                                <tr class="bg-warning text-white">
                                    <th>
                                        購買日期
                                    </th>
                                    <th>
                                        會員數量
                                    </th>
                                </tr>

                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.OrderDate)
                                        </td>
                                        <td>
                                            @item.MemberCount
                                        </td>
                                    </tr>
                                }
                            </table>
